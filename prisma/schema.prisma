generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model student {
  rollno    String   @id @db.VarChar(50)
  regno     String   @db.VarChar(50)
  name      String   @db.VarChar(200)
  year      Int
  dept      String   @db.VarChar(50)
  dob       DateTime @db.Date
  pass      String   @db.VarChar(512)
}
